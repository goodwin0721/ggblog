jdbc.driverClass = com.mysql.jdbc.Driver
#数据库地址
jdbc.jdbcUrl=jdbc:mysql://localhost:3306/#数据库名称?useUnicode=true&characterEncoding=utf-8&serverTimezone=GMT%2B8
#用户名
jdbc.user=#用户名
#密码
jdbc.password=#密码
#最大连接数
c3p0.maxPoolSize=30
#最小连接数
c3p0.minPoolSize=10
#初始化连接数10
c3p0.initialPoolSize=10
#关闭连接后不自动commit
c3p0.autoCommitOnClose=false
#获取连接超时时间
c3p0.checkoutTimeout=10000
#当获取连接失败重试次数
c3p0.acquireRetryAttempts=2
